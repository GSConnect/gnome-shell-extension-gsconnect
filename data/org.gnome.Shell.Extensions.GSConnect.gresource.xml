<?xml version="1.0" encoding="UTF-8"?>
<gresources>
  <gresource prefix="/org/gnome/Shell/Extensions/GSConnect">
    <!-- UI -->
    <file compressed="true">application.css</file>
    <file compressed="true" preprocess="xml-stripblanks">connect.ui</file>
    <file compressed="true" preprocess="xml-stripblanks">contacts.ui</file>
    <file compressed="true" preprocess="xml-stripblanks">conversation.ui</file>
    <file compressed="true" preprocess="xml-stripblanks">devel.ui</file>
    <file compressed="true" preprocess="xml-stripblanks">device.ui</file>
    <file compressed="true" preprocess="xml-stripblanks">messaging.ui</file>
    <file compressed="true" preprocess="xml-stripblanks">notification.ui</file>
    <file compressed="true" preprocess="xml-stripblanks">settings.ui</file>
    <file compressed="true" preprocess="xml-stripblanks">telephony.ui</file>
    <file compressed="true" preprocess="xml-stripblanks">enter-keyboard-shortcut.svg</file>

    <!-- DBus Service -->
    <file compressed="true">org.gnome.Shell.Extensions.GSConnect.desktop</file>
    <file compressed="true">org.gnome.Shell.Extensions.GSConnect.service</file>
    <file compressed="true" preprocess="xml-stripblanks">org.gnome.Shell.Extensions.GSConnect.xml</file>

    <!-- Bluetooth SDP -->
    <file compressed="true" preprocess="xml-stripblanks">org.gnome.Shell.Extensions.GSConnect.sdp.xml</file>

    <!-- WebExtension Manifests -->
    <file compressed="true">org.gnome.shell.extensions.gsconnect.json-chrome</file>
    <file compressed="true">org.gnome.shell.extensions.gsconnect.json-mozilla</file>

    <!-- Application Icons -->
    <file compressed="true" preprocess="xml-stripblanks">icons/org.gnome.Shell.Extensions.GSConnect.svg</file>
    <file compressed="true" preprocess="xml-stripblanks">icons/org.gnome.Shell.Extensions.GSConnect-symbolic.svg</file>

    <!-- Device Icons (Fallback) c/o GNOME -->
    <file compressed="true" preprocess="xml-stripblanks">icons/computer-symbolic.svg</file>
    <file compressed="true" preprocess="xml-stripblanks">icons/smartphone-symbolic.svg</file>
    <file compressed="true" preprocess="xml-stripblanks">icons/tv-symbolic.svg</file>

    <!-- Device Icons (Fallbacks) c/o Numix

         TODO: GNOME currently doesn't have an icon for `laptop`
               The stock icon for `tablet` is actually a wacom tablet
    -->
    <file compressed="true" preprocess="xml-stripblanks">icons/laptop-symbolic.svg</file>
    <file compressed="true" preprocess="xml-stripblanks">icons/tablet-symbolic.svg</file>

    <!-- Phonelink Icons c/o GNOME -->
    <file compressed="true" preprocess="xml-stripblanks">icons/phonelink-symbolic.svg</file>
    <file compressed="true" preprocess="xml-stripblanks">icons/phonelink-delete-symbolic.svg</file>
    <file compressed="true" preprocess="xml-stripblanks">icons/phonelink-lock-symbolic.svg</file>
    <file compressed="true" preprocess="xml-stripblanks">icons/phonelink-off-symbolic.svg</file>
    <file compressed="true" preprocess="xml-stripblanks">icons/phonelink-ring-symbolic.svg</file>
    <file compressed="true" preprocess="xml-stripblanks">icons/phonelink-setup-symbolic.svg</file>
