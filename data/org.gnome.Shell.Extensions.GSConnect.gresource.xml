  <gresource prefix="/org/gnome/Shell/Extensions/GSConnect/icons">
    <!-- Action Icons c/o GNOME -->
    <file compressed="true" preprocess="xml-stripblanks">group-avatar-symbolic.svg</file>
    <file compressed="true" preprocess="xml-stripblanks">sms-send.svg</file>
    <file compressed="true" preprocess="xml-stripblanks">sms-symbolic.svg</file>

    <!-- Browser Badges -->
    <file compressed="true">firefox-badge.png</file>
    <file compressed="true">chrome-badge.png</file>
  </gresource>
  <gresource prefix="/org/gnome/Shell/Extensions/GSConnect/gtk">
    <file compressed="true" preprocess="xml-stripblanks">menus.ui</file>
  </gresource>
</gresources>
