<interface domain="org.gnome.Shell.Extensions.GSConnect">
  <requires lib="gtk" version="4.0"/>
  <requires lib="Adw" version="1.0"/>
  
  <template class="GSConnectDeviceNavigationPage" parent="AdwNavigationPage">
    <property name="title" translatable="no">Device Page</property>
    <property name="tag">content</property>
    <property name="child">
      <object class="AdwNavigationView">
        <property name="visible">True</property>
        <child>
          <object class="AdwNavigationPage">
            <property name="title" translatable="yes">Device Settings</property>
            <property name="child">
              <object class="AdwToolbarView">
