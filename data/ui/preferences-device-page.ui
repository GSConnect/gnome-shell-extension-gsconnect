                                  <object class="AdwButtonContent">
                                    <property name="visible">True</property>
                                    <property name="label" translatable="yes">Select Folder</property>
                                    <property name="icon-name">folder-drag-accept-symbolic</property>
                                    <property name="use-underline">True</property>
                                  </object>
                                </property>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwNavigationPage">
            <property name="title" translatable="yes">Battery</property>
            <property name="tag">battery</property>
            <property name="child">
              <object class="AdwToolbarView">
                <property name="visible">True</property>
                <child type="top">
                  <object class="AdwHeaderBar">
                    <property name="visible">True</property>
                    <property name="show-title">False</property>
                  </object>
                </child>
                <property name="content">
                  <object class="AdwPreferencesPage">
                    <property name="visible">True</property>
                    <child>
                      <object class="AdwPreferencesGroup" id="battery-system">
                        <property name="visible">True</property>
                        <property name="title" translatable="yes">Device Battery</property>
                        <child>
                          <object class="AdwSwitchRow">
                            <property name="visible">True</property>
                            <property name="title" translatable="yes">Low Battery Notification</property>
                            <property name="action-name">settings.low-battery-notification</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwExpanderRow">
                            <property name="visible">True</property>
                            <property name="title" translatable="yes">Charged up to custom level notification</property>
                            <property name="action-name">settings.custom-battery-notification</property>
                            <child type="suffix">
                              <object class="GtkSwitch">
                                <property name="visible">True</property>
                                <property name="valign">center</property>
                                <property name="action-name">settings.custom-battery-notification</property>
                              </object>
                            </child>
                            <child>
                              <object class="AdwSpinRow" id="battery-custom-notification-value">
                                <property name="visible">True</property>
                                <property name="adjustment">
                                  <object class="GtkAdjustment" id="battery-custom-adjustment">
                                    <property name="upper">99</property>
                                    <property name="lower">01</property>
                                    <property name="value">80</property>
                                    <property name="step-increment">1</property>
                                    <property name="page-increment">1</property>
                                  </object>
                                </property>
                                <signal name="output" handler="_setCustomChargeLevel" swapped="no"/>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwSwitchRow">
                            <property name="visible">True</property>
                            <property name="title" translatable="yes">Fully Charged Notification</property>
                            <property name="action-name">settings.full-battery-notification</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwPreferencesGroup">
                        <property name="visible">True</property>
                        <property name="title" translatable="yes">System Battery</property>
                        <child>
                          <object class="AdwSwitchRow">
                            <property name="visible">True</property>
                            <property name="title" translatable="yes">Share Statistics</property>
                            <property name="action-name">settings.send-statistics</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwNavigationPage">
            <property name="title" translatable="yes">Commands</property>
            <property name="tag">commands</property>
            <property name="child">
              <object class="AdwToolbarView">
                <property name="visible">True</property>
                <child type="top">
                  <object class="AdwHeaderBar">
                    <property name="visible">True</property>
                    <property name="show-title">False</property>
                  </object>
                </child>
                <property name="content">
                  <object class="AdwPreferencesPage">
                    <property name="visible">True</property>
                    <child>
                      <object class="AdwPreferencesGroup" id="command-list">
                        <property name="visible">True</property>
                        <property name="title" translatable="yes">Commands</property>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwNavigationPage">
            <property name="title" translatable="yes">Notifications</property>
            <property name="tag">notifications</property>
            <property name="child">
              <object class="AdwToolbarView">
                <property name="visible">True</property>
                <child type="top">
                  <object class="AdwHeaderBar">
                    <property name="visible">True</property>
                    <property name="show-title">False</property>
                  </object>
                </child>
                <property name="content">
                  <object class="AdwPreferencesPage">
                    <property name="visible">True</property>
                    <child>
                      <object class="AdwPreferencesGroup">
                        <property name="visible">True</property>
                        <child>
                          <object class="AdwSwitchRow">
                            <property name="visible">True</property>
                            <property name="title" translatable="yes">Share Notifications</property>
                            <property name="action-name">settings.send-notifications</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwSwitchRow">
                            <property name="visible">True</property>
                            <property name="title" translatable="yes">Share When Active</property>
                            <property name="action-name">settings.send-active</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwPreferencesGroup" id="notification-apps">
                        <property name="visible">True</property>
                        <property name="title" translatable="yes">Applications</property>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwNavigationPage">
            <property name="title" translatable="yes">Telephony</property>
            <property name="tag">telephony</property>
            <property name="child">
              <object class="AdwToolbarView">
                <property name="visible">True</property>
                <child type="top">
                  <object class="AdwHeaderBar">
                    <property name="visible">True</property>
                    <property name="show-title">False</property>
                  </object>
                </child>
                <property name="content">
                  <object class="AdwPreferencesPage">
                    <property name="visible">True</property>
                    <child>
                      <object class="AdwPreferencesGroup">
                        <property name="visible">True</property>
                        <property name="title" translatable="yes">Incoming Calls</property>
                        <child>
                          <object class="AdwActionRow">
                            <property name="visible">True</property>
                            <property name="title" translatable="yes">Volume</property>
                            <child>
                              <object class="AdwToggleGroup">
                                <property name="valign">center</property>
                                <property name="vexpand">false</property>
                                <child>
                                  <object class="AdwToggle">
                                    <property name="label" translatable="yes">Nothing</property>
                                    <property name="tooltip" translatable="yes">Nothing</property>
                                    <property name="name">nothing</property>
                                  </object>
                                </child>
                                <child>
                                  <object class="AdwToggle">
                                    <property name="label" translatable="yes">Restore</property>
                                    <property name="tooltip" translatable="yes">Restore</property>
                                    <property name="name">restore</property>
                                  </object>
                                </child>
                                <child>
                                  <object class="AdwToggle">
                                    <property name="label" translatable="yes">Lower</property>
                                    <property name="tooltip" translatable="yes">Lower</property>
                                    <property name="name">lower</property>
                                  </object>
                                </child>
                                <child>
                                  <object class="AdwToggle">
                                    <property name="label" translatable="yes">Mute</property>
                                    <property name="tooltip" translatable="yes">Mute</property>
                                    <property name="name">mute</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwSwitchRow">
                            <property name="visible">True</property>
                            <property name="title" translatable="yes">Pause Media</property>
                            <property name="action-name">settings.ringing-pause</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwPreferencesGroup">
                        <property name="visible">True</property>
                        <property name="title" translatable="yes">Outgoing Calls</property>
                        <child>
                          <object class="AdwActionRow">
                            <property name="visible">True</property>
                            <property name="title" translatable="yes">Volume</property>
                            <child>
                              <object class="AdwToggleGroup">
                                <property name="valign">center</property>
                                <property name="vexpand">false</property>
                                <child>
                                  <object class="AdwToggle">
                                    <property name="label" translatable="yes">Nothing</property>
                                    <property name="tooltip" translatable="yes">Nothing</property>
                                    <property name="name">nothing</property>
                                  </object>
                                </child>
                                <child>
                                  <object class="AdwToggle">
                                    <property name="label" translatable="yes">Restore</property>
                                    <property name="tooltip" translatable="yes">Restore</property>
                                    <property name="name">restore</property>
                                  </object>
                                </child>
                                <child>
                                  <object class="AdwToggle">
                                    <property name="label" translatable="yes">Lower</property>
                                    <property name="tooltip" translatable="yes">Lower</property>
                                    <property name="name">lower</property>
                                  </object>
                                </child>
                                <child>
                                  <object class="AdwToggle">
                                    <property name="label" translatable="yes">Mute</property>
                                    <property name="tooltip" translatable="yes">Mute</property>
                                    <property name="name">mute</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwSwitchRow">
                            <property name="visible">True</property>
                            <property name="title" translatable="yes">Pause Media</property>
                            <property name="action-name">settings.talking-pause</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwSwitchRow">
                            <property name="visible">True</property>
                            <property name="title" translatable="yes">Mute Microphone</property>
                            <property name="action-name">settings.talking-microphone</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwNavigationPage">
            <property name="title" translatable="yes">Keyboard Shurtcuts</property>
            <property name="tag">keyboard_shurtcuts</property>
            <property name="child">
              <object class="AdwToolbarView">
                <property name="visible">True</property>
                <child type="top">
                  <object class="AdwHeaderBar">
                    <property name="visible">True</property>
                    <property name="show-title">False</property>
                  </object>
                </child>
                <property name="content">
                  <object class="AdwPreferencesPage">
                    <property name="visible">True</property>
                    <child>
                      <object class="AdwPreferencesGroup">
                        <property name="visible">True</property>
                        <property name="title" translatable="yes">Action Shortcuts</property>
                        <property name="header-suffix">
                          <object class="GtkButton">
                            <property name="visible">True</property>
                            <property name="child">
                              <object class="AdwButtonContent">
                                <property name="visible">True</property>
                                <property name="icon-name">view-refresh-symbolic</property>
                                <property name="label" translatable="yes">Reset All…</property>
                                <property name="use-underline">True</property>
                              </object>
                            </property>
                          </object>
                        </property>
                        <child>
                          <!-- this is needed for the shortcuts menu -->
                          <object class="GtkListBox" id="shortcuts-actions-list">
                            <style>
                              <class name="boxed-list"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwNavigationPage">
            <property name="title" translatable="yes">Advanced</property>
            <property name="tag">advanced</property>
            <property name="child">
              <object class="AdwToolbarView">
                <property name="visible">True</property>
                <child type="top">
                  <object class="AdwHeaderBar">
                    <property name="visible">True</property>
                    <property name="show-title">False</property>
                  </object>
                </child>
                <property name="content">
                  <object class="AdwPreferencesPage">
                    <property name="visible">True</property>
                    <child>
                      <object class="AdwPreferencesGroup" id="plugin-list">
                        <property name="visible">True</property>
                        <property name="title" translatable="yes">Plugins</property>
                      </object>
                    </child>
                    <child>
                      <object class="AdwPreferencesGroup" id="experimental-list">
                        <property name="visible">True</property>
                        <property name="title" translatable="yes">Experimental</property>
                        <child>
                          <object class="AdwActionRow" id="device-cache-row">
                            <property name="height-request">56</property>
                            <property name="visible">True</property>
                            <property name="selectable">False</property>
                            <property name="title">Device Cache</property>
                            <child>
                              <object class="GtkButton" id="device-cache">
                                <property name="visible">True</property>
                                <property name="halign">end</property>
                                <property name="valign">center</property>
                                <property name="action-name">device.clearCache</property>
                                <property name="label" translatable="yes">Clear Cache…</property>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwSwitchRow" id="legacy-sms-row">
                            <property name="height-request">56</property>
                            <property name="visible">True</property>
