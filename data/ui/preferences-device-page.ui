                            <property name="action-name">settings.legacy-sms</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwSwitchRow" id="sftp">
                            <property name="height-request">56</property>
                            <property name="visible">True</property>
                            <property name="selectable">False</property>
                            <property name="title">SFTP Automount</property>
                            <property name="action-name">settings.automount</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </property>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
